// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Item {
  id           Int    @id
  title        String
  url          String
  image        String
  price        Float
  description  String
  reviews_qty  Int
  review_score Int
}
